
log4rpg.debug=off, printer

log4rpg.rootLogger=DEBUG, member
log4rpg.logger.de.tools400=INFO

log4rpg.appender.member=*LIBL/LOG4RLMAPP(RollingMemberAppender)
log4rpg.appender.member.file.name=LOG4_LOG
log4rpg.appender.member.file.library=RADDATZ
log4rpg.appender.member.maxnumrcds=128
log4rpg.appender.member.maxfilesize=10485760
log4rpg.appender.member.textlength=1024
log4rpg.appender.member.layout.conversionPattern=%z [%-5p] %L/%P(%M).%F (%S) %m%n






